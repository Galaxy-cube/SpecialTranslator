<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="generalNoTitle" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/metal</item>
        <item name="colorPrimaryDark">@color/pewter</item>
        <item name="colorPrimaryVariant">@color/kraftPaper</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/c5</item>
        <item name="colorSecondaryVariant">@color/c4</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#303523</item>
        <item name="android:windowNoTitle">true</item>
    </style>
</resources>